{
  "compilerOptions": {
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "target": "es5",
    "jsx": "react",
    "moduleResolution": "Classic",
    "module": "AMD",
    "skipLibCheck": true,
    "allowJs": true,
    "allowUnreachableCode": true,
    "outDir": "dist"
  },
  "exclude": [
    "node_modules",
    "wwwroot"
  ]
}
